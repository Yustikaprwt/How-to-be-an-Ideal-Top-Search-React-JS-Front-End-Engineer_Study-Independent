{
  "resource_version": 5,
  "metadata": {
    "version": 3,
    "sources": [
      {
        "name": "kampus_merdeka",
        "kind": "postgres",
        "tables": [
          {
            "table": {
              "name": "anggota",
              "schema": "public"
            }
          },
          {
            "table": {
              "name": "keterangan",
